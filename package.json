{
        "name": "projectpages",
        "version": "1.16.0",
        "repository": {
                "type": "git",
                "url": "https://github.com/wridgeu/github_pages.git",
                "directory": "webapp/"
        },
        "homepage": "https://wridgeu.github.io/",
        "engines": {
                "node": "16.x.x"
        },
        "scripts": {
                "start": "ui5 serve -o index.html",
                "start-build": "ui5 serve -o index.html --config ui5-dist.yaml",
                "serve-unit": "ui5 serve --port 8081 --open /test/unit/unitTests.qunit.html",
                "test": "npm run lint && npm run karma",
                "karma-ci": "karma start karma-ci.conf.js",
                "karma": "rimraf coverage && npm run karma-ci",
                "lint": "eslint webapp",
                "build": "ui5 build self-contained --all --dest ./dist",
                "build-no-debug": "ui5 build self-contained --all --exclude-task=createDebugFiles generateManifestBundle --clean-dest true --dest ./dist"
        },
        "dependencies": {
                "@highlightjs/cdn-assets": "^11.3.1",
                "marked": "^4.0.1",
                "shx": "^0.3.3"
        },
        "devDependencies": {
                "@openui5/ts-types": "^1.90.8",
                "@ui5/cli": "^2.14.0",
                "eslint": "^8.2.0",
                "karma": "^6.3.8",
                "karma-chrome-launcher": "^3.1.0",
                "karma-coverage": "^2.0.3",
                "karma-ui5": "^2.3.4",
                "ui5-middleware-livereload": "^0.5.8",
                "ui5-task-minifier": "^1.0.2"
        },
        "ui5": {
                "dependencies": [
                        "@highlightjs/cdn-assets",
                        "marked",
                        "ui5-middleware-livereload",
                        "ui5-task-minifier"
                ]
        }
}

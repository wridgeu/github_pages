{
    "name": "projectpages",
    "version": "1.20.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/wridgeu/github_pages.git"
    },
    "homepage": "https://wridgeu.github.io/",
    "engines": {
        "node": "16.x.x"
    },
    "scripts": {
        "build": "npm-run-all build:ts build:ui5",
        "build:ts": "babel src --out-dir webapp --source-maps inline --extensions \".ts,.js\" --copy-files",
        "build:ui5": "ui5 build --clean-dest",
        "watch:ts": "babel src --out-dir webapp --source-maps inline --extensions \".ts,.js\" --copy-files --watch",
        "start:ui5": "ui5 serve --port 8080 -o index.html",
        "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
        "start:build": "ui5 serve -o index.html --config ui5-dist.yaml",
        "start:ui5:bg": "ui5 serve --port 8080",
        "ts-typecheck": "tsc --noEmit",
        "lint": "eslint src",
        "test": "start-server-and-test start:ui5:bg http://localhost:8080 wdio",
        "test:bg": "HEADLESS=true start-server-and-test start:ui5:bg http://localhost:8080 wdio",
        "wdio": "wdio run wdio.conf.ts",
        "build:no:dbg": "ui5 build self-contained --all --exclude-task=createDebugFiles --clean-dest true --dest ./dist"
    },
    "dependencies": {
        "@highlightjs/cdn-assets": "^11.4.0",
        "highlight.js": "^11.4.0",
        "marked": "^4.0.12",
        "shx": "^0.3.4"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.6",
        "@babel/core": "^7.17.5",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-typescript": "^7.16.7",
        "@openui5/ts-types-esm": "1.99.0",
        "@types/highlightjs": "^9.12.2",
        "@types/jquery": "3.5.14",
        "@types/marked": "^4.0.2",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@ui5/cli": "^2.14.6",
        "@wdio/cli": "^7.16.16",
        "@wdio/local-runner": "^7.16.16",
        "@wdio/mocha-framework": "^7.16.15",
        "@wdio/spec-reporter": "^7.16.14",
        "babel-preset-transform-ui5": "^7.0.5",
        "chromedriver": "^98.0.1",
        "eslint": "^8.9.0",
        "npm-run-all": "^4.1.5",
        "start-server-and-test": "^1.14.0",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5",
        "ui5-middleware-livereload": "^0.5.9",
        "ui5-task-minifier": "^1.0.2",
        "ui5-tooling-modules": "^0.2.0",
        "wdio-chromedriver-service": "^7.2.8",
        "wdio-ui5-service": "^0.8.2"
    },
    "ui5": {
        "dependencies": [
            "@highlightjs/cdn-assets",
            "highlight.js",
            "marked",
            "ui5-middleware-livereload",
            "ui5-task-minifier",
            "ui5-tooling-modules"
        ]
    }
}

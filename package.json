{
    "name": "projectpages",
    "version": "1.21.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/wridgeu/github_pages.git"
    },
    "homepage": "https://wridgeu.github.io/",
    "engines": {
        "node": "16.x.x"
    },
    "scripts": {
        "build": "npm-run-all build:ts build:ui5",
        "build:ts": "babel src --out-dir webapp --source-maps inline --extensions \".ts,.js\" --copy-files",
        "build:ui5": "ui5 build self-contained --all --exclude-task=createDebugFiles --clean-dest",
        "watch:ts": "npm-run-all --parallel start:ts start:ui5",
        "start:ts": "babel src --out-dir webapp --source-maps inline --extensions \".ts,.js\" --copy-files --watch",
        "start:ui5": "ui5 serve --port 8080 -o index.html",
        "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
        "start:build": "ui5 serve -o index.html --config ui5-dist.yaml",
        "ts-typecheck": "tsc --noEmit",
        "lint": "eslint src",
        "test": "start-server-and-test start:ui5:bg http://localhost:8080 wdio",
        "test:bg": "HEADLESS=true start-server-and-test start:ui5:bg http://localhost:8080 wdio",
        "wdio": "wdio run wdio.conf.ts",
        "build:no:dbg": "ui5 build self-contained --all --exclude-task=createDebugFiles --clean-dest true --dest ./dist"
    },
    "dependencies": {
        "highlight.js": "^11.6.0",
        "marked": "^4.0.18",
        "shx": "^0.3.4",
        "ui5-middleware-livereload": "^0.6.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.9",
        "@babel/core": "^7.18.9",
        "@babel/preset-env": "^7.18.9",
        "@babel/preset-typescript": "^7.18.6",
        "@openui5/ts-types-esm": "1.104.0",
        "@types/highlightjs": "^9.12.2",
        "@types/jquery": "3.5.14",
        "@types/marked": "^4.0.3",
        "@types/node": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.30.7",
        "@typescript-eslint/parser": "^5.30.7",
        "@ui5/cli": "^2.14.9",
        "@wdio/cli": "^7.20.7",
        "@wdio/local-runner": "^7.20.7",
        "@wdio/mocha-framework": "^7.20.7",
        "@wdio/spec-reporter": "^7.20.7",
        "babel-preset-transform-ui5": "^7.0.5",
        "chromedriver": "^103.0.0",
        "eslint": "^8.20.0",
        "npm-run-all": "^4.1.5",
        "start-server-and-test": "^1.14.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4",
        "ui5-task-minifier": "^1.0.2",
        "ui5-tooling-modules": "^0.6.0",
        "wdio-chromedriver-service": "^7.3.2",
        "wdio-ui5-service": "^0.9.9"
    },
    "ui5": {
        "dependencies": [
            "marked",
            "ui5-middleware-livereload",
            "ui5-task-minifier",
            "ui5-tooling-modules"
        ]
    }
}

{
  "name": "projectpages",
  "version": "1.25.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wridgeu/github_pages.git"
  },
  "homepage": "https://wridgeu.github.io/",
  "engines": {
    "node": "18.x.x"
  },
  "scripts": {
    "build": "ui5 build self-contained --all --include-task=generateVersionInfo --clean-dest",
    "start": "ui5 serve --port 8080 -o index.html",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "start:build": "ui5 serve -o index.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "lint": "eslint webapp",
    "test": "start-server-and-test start:ui5:bg http://localhost:8080 wdio",
    "test:bg": "HEADLESS=true start-server-and-test start:ui5:bg http://localhost:8080 wdio",
    "wdio": "wdio run wdio.conf.ts",
    "build:deploy": "ui5 build self-contained --all --include-task=generateVersionInfo --clean-dest --dest ./dist"
  },
  "dependencies": {
    "highlight.js": "^11.8.0",
    "marked": "^4.3.0",
    "shx": "^0.3.4"
  },
  "devDependencies": {
    "@openui5/ts-types-esm": "1.113.0",
    "@types/highlightjs": "^9.12.2",
    "@types/jquery": "3.5.16",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@ui5/cli": "^3.1.1",
    "@wdio/cli": "^8.11.2",
    "@wdio/local-runner": "^8.8.8",
    "@wdio/mocha-framework": "^8.8.7",
    "@wdio/spec-reporter": "^8.8.7",
    "chromedriver": "^112.0.0",
    "eslint": "^8.39.0",
    "pnpm": "^8.3.1",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "ui5-middleware-livereload": "^0.8.2",
    "ui5-task-minifier": "^1.0.2",
    "ui5-tooling-modules": "^0.9.7",
    "ui5-tooling-transpile": "^0.7.8",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-ui5-service": "^1.3.3"
  }
}

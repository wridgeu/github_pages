{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"preserveConstEnums": true,
		"sourceMap": false,
		"allowJs": true,
		"strict": true,
		"strictNullChecks": false,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"strictPropertyInitialization": false,
		"rootDir": "./webapp",
		"outDir": "./dist",
		"baseUrl": "./",
		"types": [
			"@openui5/types"
		],
		"paths": {
			"sapmarco/projectpages/*": [
				"./webapp/*"
			]
		}
	},
	"include": [
		"./webapp/**/*"
	]
}
{
	"compilerOptions": {
		"target": "es2015",
		"module": "es2015",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"preserveConstEnums": true,
		"sourceMap": false,
		"allowJs": true,
		"strict": true,
		"strictNullChecks": false,
		"importHelpers": true,
		"forceConsistentCasingInFileNames": true,
		"strictPropertyInitialization": false,
		"rootDir": "./src",
		"outDir": "./dist",
		"baseUrl": "./",
		"typeRoots": [
			"./node_modules/@types/"
		],
		"types": [
			"@openui5/ts-types-esm"
		],
		"paths": {
			"sapmarco/projectpages/*": [
				"./src/*"
			]
		}
	},
	"include": [
		"./src/**/*"
	]
}